{
    "image":"mcr.microsoft.com/devcontainers/javascript-node:1-22-bookworm",
    "portsAttributes": {
        "3000": {
        "label": "Application",
        "onAutoForward": "openPreview"
        }
    },
    "forwardPorts": [3000],
    "remoteEnv": {
        "REGION_CLIENT_ID": "${localEnv:REGION_CLIENT_ID}",
        "REGION_CLIENT_SECRET": "${localEnv:REGION_CLIENT_SECRET}"
    }
}
